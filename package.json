{
  "name": "tw_scoring",
  "version": "1.1.5",
  "author": "Insyn, <insyner@gmail.com>",
  "description": "Scoring app by TimingWeb",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && node .electron-vue/build.js && electron-builder",
    "build:dir": "set NODE_OPTIONS=--openssl-legacy-provider && node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "TW Scoring",
    "appId": "TW Scoring Secretary Terminal",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "app_assets",
        "to": "app_assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icons/icon.ico"
    },
    "nsis": {
      "installerIcon": "build/icons/icon.ico",
      "uninstallerIcon": "build/icons/icon.ico",
      "uninstallDisplayName": "TW Scoring",
      "oneClick": true,
      "perMachine": false
    },
    "linux": {
      "icon": "build/icons/icon-256x256.png",
      "category": "Office",
      "target": [
        "AppImage",
        "snap",
        "deb"
      ]
    }
  },
  "dependencies": {
    "axios": "~0.18.1",
    "csv": "~6.0.5",
    "express": "^4.18.2",
    "html2pdf.js": "~0.9.3",
    "papaparse": "^5.3.2",
    "pdf-creator-node": "~1.4.3",
    "read-excel-file": "~4.1.0",
    "socket.io": "~3.1.2",
    "socket.io-client": "~3.1.3",
    "systeminformation": "^5.12.13",
    "uuid": "~9.0.0",
    "v8-compile-cache": "~2.3.0",
    "vue": "~2.7.14",
    "vue-electron": "~1.0.6",
    "vue-router": "~3.6.5",
    "vue-socket.io": "~3.0.10",
    "vuetify": "~2.6.12",
    "vuex": "~3.6.2",
    "vuex-electron": "~1.0.3",
    "xml-js": "~1.6.11"
  },
  "devDependencies": {
    "@mdi/js": "~5.9.55",
    "ajv": "^6.12.6",
    "babel-core": "~6.26.3",
    "babel-loader": "~7.1.5",
    "babel-minify-webpack-plugin": "~0.3.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "cfonts": "^2.10.1",
    "chalk": "~2.4.2",
    "copy-webpack-plugin": "~4.6.0",
    "cross-env": "~5.2.1",
    "css-loader": "~0.28.11",
    "deepmerge": "~4.2.2",
    "del": "~3.0.0",
    "devtron": "~1.4.0",
    "electron": "^2.0.18",
    "electron-builder": "~20.44.4",
    "electron-debug": "~1.5.0",
    "electron-devtools-installer": "~2.2.4",
    "file-loader": "~1.1.11",
    "html-webpack-plugin": "~3.2.0",
    "material-design-icons-iconfont": "~6.7.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mongoose": "^5.13.15",
    "multispinner": "~0.2.1",
    "node-loader": "~0.6.0",
    "polyfill-object.fromentries": "^1.0.1",
    "sass": "~1.51.0",
    "sass-loader": "~10.2.1",
    "style-loader": "~0.21.0",
    "url-loader": "~1.1.2",
    "vue-cli-plugin-electron-builder": "~2.0.0",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-html-loader": "~1.2.4",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "~4.1.3",
    "vue-template-compiler": "~2.5.22",
    "webpack": "~4.46.0",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.11.3",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "~4.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "~4.0.6",
    "utf-8-validate": "~5.0.10"
  }
}
